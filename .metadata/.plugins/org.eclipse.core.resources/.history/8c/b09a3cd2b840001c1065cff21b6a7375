#include <stdio.h>

int main() {
	char nome[10];
	char senha[10];

	printf("Digite o nome do usuário: ");
	fflush(stdout);
	scanf("%s", &nome);

	printf("Digite a senha do usuário: ");
	fflush(stdout);
	scanf("%s", &senha);

	while (nome == senha) {
		printf("A senha não pode ser igual ao nome");

		printf("Digite o nome do usuário: ");
		fflush(stdout);
		scanf("%s", &nome);

		printf("Digite a senha do usuário: ");
		fflush(stdout);
		scanf("%s", &senha);
	}

	printf("Login realizado com sucesso");

}
