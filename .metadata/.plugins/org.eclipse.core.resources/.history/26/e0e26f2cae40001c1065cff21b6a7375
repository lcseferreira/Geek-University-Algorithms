// INCLUINDO A BIBLIOTECA PADRÃO std (standard) io (input/output)
#include <stdio.h>

int main () {
	// DECLARANDO VARIÁVEIS
	int n1, n2, n3, n4, q1, q2, q3, q4;

	// ENTRADA
	printf("Digite um número: ");
	fflush(stdout);
	scanf("%d", &n1);
	q1 = n1 * n1;

	printf("Digite um número: ");
	fflush(stdout);
	scanf("%d", &n2);
	q2 = n2 * n2;

	printf("Digite um número: ");
	fflush(stdout);
	scanf("%d", &n3);
	q3 = n3 * n3;

	printf("Digite um número: ");
	fflush(stdout);
	scanf("%d", &n4);
	q4 = n4 * n4;

	if (q3 >= 1000) {
		printf("%d", q3);
	} else {
		printf("%d", q1);
		printf("%d", q2);
		printf("%d", q3);
		printf("%d", q4);
	}
}
