// INCLUINDO A BIBLIOTECA PADRÃO std (standard) io (input/output)
#include <stdio.h>

int main () {
	// DECLARANDO VARIÁVEIS
	int n1, n2, n3, n4, q1, q2, q3, q4;

	// ENTRADA
	printf("Digite um número: ");
	fflush(stdout);
	scanf("%f", &n1);
	q1 = n1 ** 2;

	printf("Digite um número: ");
	fflush(stdout);
	scanf("%f", &n2);
	q2 = n2 ** 2;

	printf("Digite um número: ");
	fflush(stdout);
	scanf("%f", &n3);
	q3 = n3 ** 2;

	printf("Digite um número: ");
	fflush(stdout);
	scanf("%f", &n4);
	q4 = n4 ** 2;

	if (q3 >= 1000) {
		printf("%d", q3);
	} else {
		printf("%d", q1, "\n");
		printf("%d", q2, "\n");
		printf("%d", q3, "\n");
		printf("%d", q4, "\n");
	}
}
